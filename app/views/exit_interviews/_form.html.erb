<%= simple_form_for(@exit_interview) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :note %>
  </div>

  <div class="form-inputs">
    <%= f.input :tag_list %>
    <p class="help-block"><em>Keywords seperated by commas.</em></p>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
